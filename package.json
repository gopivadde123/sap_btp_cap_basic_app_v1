{
  "name": "my_first_btp_app",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1.11.1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "stratagy": "mocked",
          "users": {
            "gopi": {
              "roles": [
                "Viewer",
                "Editor",
                "Admin"
              ],
              "userAttributes": {
                "EmpLevel": "Mid-Emp"
              }
            }
          }
        },
        "kind": "xsuaa"
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
