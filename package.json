{
  "name": "my_first_btp_app",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4.11.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1.11.1",
    "@sap/cds-dk": "^8",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-manageempdpt": "cds watch --open manageempdpt/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "stratagy": "mocked",
          "users": {
            "gopi": {
              "roles": [
                "Viewer",
                "Editor",
                "Admin"
              ],
              "userAttributes": {
                "EmpLevel": [
                  "Mid-Emp",
                  "Junior-Emp"
                ]
              }
            }
          }
        },
        "kind": "xsuaa"
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/manageempdpt"
  ]
}
